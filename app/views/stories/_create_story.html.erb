<%= form_for @story, :html => {:multipart => false} do |f| %>
  <%= f.text_field :caption, class: "form-control", placeholder: "What's on your mind?", required: true %><br>
  <%= f.file_field :story_pic, class: "form-control", required: true %><br>
  <%= f.submit "Post Story", class: "btn btn-primary" %>
<% end %>
